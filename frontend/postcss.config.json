{
  "plugins": [
    "postcss-import",
    "postcss-simple-vars",
    "postcss-flexbugs-fixes",
    [
      "postcss-preset-env",
      {
        "autoprefixer": {
          "flexbox": "no-2009"
        },
        "stage": 3,
        "features": {
          "custom-properties": false
        }
      }
    ],
    [
      "postcss-rem",
      {
        "baseline": 14,
        "fallback": true
      }
    ]
  ]
}
